<%- breadcrumb :edit_marketplace_product, product %>

<%= render "form", product: product %>


<%- if product.archived? %>
  <%= render CardComponent.new(classes: "mt-6") do %>
    <%= button_to("Restore", product.location, method: :put, params: { product: { restore: true } }, class: "w-full") %>
  <%- end %>
<%- end %>
